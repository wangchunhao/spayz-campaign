[32m+[m[32m//[m
[32m+[m[32m// Navs[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Base class[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m.nav {[m
[32m+[m[32m  margin-bottom: 0;[m
[32m+[m[32m  padding-left: 0; // Override default ul/ol[m
[32m+[m[32m  list-style: none;[m
[32m+[m[32m  &:extend(.clearfix all);[m
[32m+[m
[32m+[m[32m  > li {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    display: block;[m
[32m+[m
[32m+[m[32m    > a {[m
[32m+[m[32m      position: relative;[m
[32m+[m[32m      display: block;[m
[32m+[m[32m      padding: @nav-link-padding;[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        text-decoration: none;[m
[32m+[m[32m        background-color: @nav-link-hover-bg;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Disabled state sets text to gray and nukes hover/tab effects[m
[32m+[m[32m    &.disabled > a {[m
[32m+[m[32m      color: @nav-disabled-link-color;[m
[32m+[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: @nav-disabled-link-hover-color;[m
[32m+[m[32m        text-decoration: none;[m
[32m+[m[32m        background-color: transparent;[m
[32m+[m[32m        cursor: @cursor-disabled;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Open dropdowns[m
[32m+[m[32m  .open > a {[m
[32m+[m[32m    &,[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      background-color: @nav-link-hover-bg;[m
[32m+[m[32m      border-color: @link-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Nav dividers (deprecated with v3.0.1)[m
[32m+[m[32m  //[m
[32m+[m[32m  // This should have been removed in v3 with the dropping of `.nav-list`, but[m
[32m+[m[32m  // we missed it. We don't currently support this anywhere, but in the interest[m
[32m+[m[32m  // of maintaining backward compatibility in case you use it, it's deprecated.[m
[32m+[m[32m  .nav-divider {[m
[32m+[m[32m    .nav-divider();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Prevent IE8 from misplacing imgs[m
[32m+[m[32m  //[m
[32m+[m[32m  // See https://github.com/h5bp/html5-boilerplate/issues/984#issuecomment-3985989[m
[32m+[m[32m  > li > a > img {[m
[32m+[m[32m    max-width: none;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Tabs[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m// Give the tabs something to sit on[m
[32m+[m[32m.nav-tabs {[m
[32m+[m[32m  border-bottom: 1px solid @nav-tabs-border-color;[m
[32m+[m[32m  > li {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    // Make the list-items overlay the bottom border[m
[32m+[m[32m    margin-bottom: -1px;[m
[32m+[m
[32m+[m[32m    // Actual tabs (as links)[m
[32m+[m[32m    > a {[m
[32m+[m[32m      margin-right: 2px;[m
[32m+[m[32m      line-height: @line-height-base;[m
[32m+[m[32m      border: 1px solid transparent;[m
[32m+[m[32m      border-radius: @border-radius-base @border-radius-base 0 0;[m
[32m+[m[32m      &:hover {[m
[32m+[m[32m        border-color: @nav-tabs-link-hover-border-color @nav-tabs-link-hover-border-color @nav-tabs-border-color;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Active state, and its :hover to override normal :hover[m
[32m+[m[32m    &.active > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: @nav-tabs-active-link-hover-color;[m
[32m+[m[32m        background-color: @nav-tabs-active-link-hover-bg;[m
[32m+[m[32m        border: 1px solid @nav-tabs-active-link-hover-border-color;[m
[32m+[m[32m        border-bottom-color: transparent;[m
[32m+[m[32m        cursor: default;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  // pulling this in mainly for less shorthand[m
[32m+[m[32m  &.nav-justified {[m
[32m+[m[32m    .nav-justified();[m
[32m+[m[32m    .nav-tabs-justified();[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
