[32m+[m[32m//[m
[32m+[m[32m// Pagination (multiple pages)[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m[32m.pagination {[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  padding-left: 0;[m
[32m+[m[32m  margin: @line-height-computed 0;[m
[32m+[m[32m  border-radius: @border-radius-base;[m
[32m+[m
[32m+[m[32m  > li {[m
[32m+[m[32m    display: inline; // Remove list-style and block-level defaults[m
[32m+[m[32m    > a,[m
[32m+[m[32m    > span {[m
[32m+[m[32m      position: relative;[m
[32m+[m[32m      float: left; // Collapse white-space[m
[32m+[m[32m      padding: @padding-base-vertical @padding-base-horizontal;[m
[32m+[m[32m      line-height: @line-height-base;[m
[32m+[m[32m      text-decoration: none;[m
[32m+[m[32m      color: @pagination-color;[m
[32m+[m[32m      background-color: @pagination-bg;[m
[32m+[m[32m      border: 1px solid @pagination-border;[m
[32m+[m[32m      margin-left: -1px;[m
[32m+[m[32m    }[m
[32m+[m[32m    &:first-child {[m
[32m+[m[32m      > a,[m
[32m+[m[32m      > span {[m
[32m+[m[32m        margin-left: 0;[m
[32m+[m[32m        .border-left-radius(@border-radius-base);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    &:last-child {[m
[32m+[m[32m      > a,[m
[32m+[m[32m      > span {[m
[32m+[m[32m        .border-right-radius(@border-radius-base);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > li > a,[m
[32m+[m[32m  > li > span {[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      color: @pagination-hover-color;[m
[32m+[m[32m      background-color: @pagination-hover-bg;[m
[32m+[m[32m      border-color: @pagination-hover-border;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .active > a,[m
[32m+[m[32m  > .active > span {[m
[32m+[m[32m    &,[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      z-index: 2;[m
[32m+[m[32m      color: @pagination-active-color;[m
[32m+[m[32m      background-color: @pagination-active-bg;[m
[32m+[m[32m      border-color: @pagination-active-border;[m
[32m+[m[32m      cursor: default;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .disabled {[m
[32m+[m[32m    > span,[m
[32m+[m[32m    > span:hover,[m
[32m+[m[32m    > span:focus,[m
[32m+[m[32m    > a,[m
[32m+[m[32m    > a:hover,[m
[32m+[m[32m    > a:focus {[m
[32m+[m[32m      color: @pagination-disabled-color;[m
[32m+[m[32m      background-color: @pagination-disabled-bg;[m
[32m+[m[32m      border-color: @pagination-disabled-border;[m
[32m+[m[32m      cursor: @cursor-disabled;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
