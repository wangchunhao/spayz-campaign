[32m+[m[32m      margin-bottom: 5px;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .dropdown .dropdown-menu {[m
[32m+[m[32m    top: auto;[m
[32m+[m[32m    left: auto;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (min-width: @screen-sm-min) {[m
[32m+[m[32m    > li {[m
[32m+[m[32m      display: table-cell;[m
[32m+[m[32m      width: 1%;[m
[32m+[m[32m      > a {[m
[32m+[m[32m        margin-bottom: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Move borders to anchors instead of bottom of list[m
[32m+[m[32m//[m
[32m+[m[32m// Mixin for adding on top the shared `.nav-justified` styles for our tabs[m
[32m+[m[32m.nav-tabs-justified {[m
[32m+[m[32m  border-bottom: 0;[m
[32m+[m
[32m+[m[32m  > li > a {[m
[32m+[m[32m    // Override margin from .nav-tabs[m
[32m+[m[32m    margin-right: 0;[m
[32m+[m[32m    border-radius: @border-radius-base;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .active > a,[m
[32m+[m[32m  > .active > a:hover,[m
[32m+[m[32m  > .active > a:focus {[m
[32m+[m[32m    border: 1px solid @nav-tabs-justified-link-border-color;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (min-width: @screen-sm-min) {[m
[32m+[m[32m    > li > a {[m
[32m+[m[32m      border-bottom: 1px solid @nav-tabs-justified-link-border-color;[m
[32m+[m[32m      border-radius: @border-radius-base @border-radius-base 0 0;[m
[32m+[m[32m    }[m
[32m+[m[32m    > .active > a,[m
[32m+[m[32m    > .active > a:hover,[m
[32m+[m[32m    > .active > a:focus {[m
[32m+[m[32m      border-bottom-color: @nav-tabs-justified-active-link-border-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Tabbable tabs[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m// Hide tabbable panes to start, show them when `.active`[m
[32m+[m[32m.tab-content {[m
[32m+[m[32m  > .tab-pane {[m
[32m+[m[32m    display: none;[m
[32m+[m[32m    visibility: hidden;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .active {[m
[32m+[m[32m    display: block;[m
[32m+[m[32m    visibility: visible;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Dropdowns[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m// Specific dropdowns[m
[32m+[m[32m.nav-tabs .dropdown-menu {[m
[32m+[m[32m  // make dropdown border overlap tab border[m
[32m+[m[32m  margin-top: -1px;[m
[32m+[m[32m  // Remove the top rounded corners here since there is a hard edge above the menu[m
[32m+[m[32m  .border-top-radius(0);[m
[32m+[m[32m}[m
