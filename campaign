[32m+[m
[32m+[m[32m// Navbar nav links[m
[32m+[m[32m//[m
[32m+[m[32m// Builds on top of the `.nav` components with its own modifier class to make[m
[32m+[m[32m// the nav the full height of the horizontal nav (above 768px).[m
[32m+[m
[32m+[m[32m.navbar-nav {[m
[32m+[m[32m  margin: (@navbar-padding-vertical / 2) -@navbar-padding-horizontal;[m
[32m+[m
[32m+[m[32m  > li > a {[m
[32m+[m[32m    padding-top:    10px;[m
[32m+[m[32m    padding-bottom: 10px;[m
[32m+[m[32m    line-height: @line-height-computed;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (max-width: @grid-float-breakpoint-max) {[m
[32m+[m[32m    // Dropdowns get custom display when collapsed[m
[32m+[m[32m    .open .dropdown-menu {[m
[32m+[m[32m      position: static;[m
[32m+[m[32m      float: none;[m
[32m+[m[32m      width: auto;[m
[32m+[m[32m      margin-top: 0;[m
[32m+[m[32m      background-color: transparent;[m
[32m+[m[32m      border: 0;[m
[32m+[m[32m      box-shadow: none;[m
[32m+[m[32m      > li > a,[m
[32m+[m[32m      .dropdown-header {[m
[32m+[m[32m        padding: 5px 15px 5px 25px;[m
[32m+[m[32m      }[m
[32m+[m[32m      > li > a {[m
[32m+[m[32m        line-height: @line-height-computed;[m
[32m+[m[32m        &:hover,[m
[32m+[m[32m        &:focus {[m
[32m+[m[32m          background-image: none;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Uncollapse the nav[m
[32m+[m[32m  @media (min-width: @grid-float-breakpoint) {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    margin: 0;[m
[32m+[m
[32m+[m[32m    > li {[m
[32m+[m[32m      float: left;[m
[32m+[m[32m      > a {[m
[32m+[m[32m        padding-top:    @navbar-padding-vertical;[m
[32m+[m[32m        padding-bottom: @navbar-padding-vertical;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Navbar form[m
[32m+[m[32m//[m
[32m+[m[32m// Extension of the `.form-inline` with some extra flavor for optimum display in[m
[32m+[m[32m// our navbars.[m
[32m+[m
[32m+[m[32m.navbar-form {[m
[32m+[m[32m  margin-left: -@navbar-padding-horizontal;[m
[32m+[m[32m  margin-right: -@navbar-padding-horizontal;[m
[32m+[m[32m  padding: 10px @navbar-padding-horizontal;[m
[32m+[m[32m  border-top: 1px solid transparent;[m
[32m+[m[32m  border-bottom: 1px solid transparent;[m
[32m+[m[32m  @shadow: inset 0 1px 0 rgba(255,255,255,.1), 0 1px 0 rgba(255,255,255,.1);[m
[32m+[m[32m  .box-shadow(@shadow);[m
[32m+[m
[32m+[m[32m  // Mixin behavior for optimum display[m
[32m+[m[32m  .form-inline();[m
[32m+[m
[32m+[m[32m  .form-group {[m
[32m+[m[32m    @media (max-width: @grid-float-breakpoint-max) {[m
[32m+[m[32m      margin-bottom: 5px;[m
[32m+[m
[32m+[m[32m      &:last-child {[m
[32m+[m[32m        margin-bottom: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Vertically center in expanded, horizontal navbar[m
[32m+[m[32m  .navbar-vertical-align(@input-height-base);[m
[32m+[m
[32m+[m[32m  // Undo 100% width for pull classes[m
[32m+[m[32m  @media (min-width: @grid-float-breakpoint) {[m
[32m+[m[32m    width: auto;[m
[32m+[m[32m    border: 0;[m
[32m+[m[32m    margin-left: 0;[m
[32m+[m[32m    margin-right: 0;[m
[32m+[m[32m    padding-top: 0;[m
[32m+[m[32m    padding-bottom: 0;[m
[32m+[m[32m    .box-shadow(none);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
