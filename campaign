[32m+[m[32m//[m
[32m+[m[32m// Breadcrumbs[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m.breadcrumb {[m
[32m+[m[32m  padding: @breadcrumb-padding-vertical @breadcrumb-padding-horizontal;[m
[32m+[m[32m  margin-bottom: @line-height-computed;[m
[32m+[m[32m  list-style: none;[m
[32m+[m[32m  background-color: @breadcrumb-bg;[m
[32m+[m[32m  border-radius: @border-radius-base;[m
[32m+[m
[32m+[m[32m  > li {[m
[32m+[m[32m    display: inline-block;[m
[32m+[m
[32m+[m[32m    + li:before {[m
[32m+[m[32m      content: "@{breadcrumb-separator}\00a0"; // Unicode space added since inline-block means non-collapsing white-space[m
[32m+[m[32m      padding: 0 5px;[m
[32m+[m[32m      color: @breadcrumb-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .active {[m
[32m+[m[32m    color: @breadcrumb-active-color;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/resources/assets/less/bootstrap/button-groups.less b/resources/assets/less/bootstrap/button-groups.less[m
[1mnew file mode 100644[m
[1mindex 0000000..f84febb[m
[1m--- /dev/null[m
[1m+++ b/resources/assets/less/bootstrap/button-groups.less[m
[36m@@ -0,0 +1,243 @@[m
[32m+[m[32m//[m
[32m+[m[32m// Button groups[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m// Make the div behave like a button[m
[32m+[m[32m.btn-group,[m
[32m+[m[32m.btn-group-vertical {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  vertical-align: middle; // match .btn alignment given font-size hack above[m
[32m+[m[32m  > .btn {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    // Bring the "active" button to the front[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus,[m
[32m+[m[32m    &:active,[m
[32m+[m[32m    &.active {[m
[32m+[m[32m      z-index: 2;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Prevent double borders when buttons are next to each other[m
[32m+[m[32m.btn-group {[m
[32m+[m[32m  .btn + .btn,[m
[32m+[m[32m  .btn + .btn-group,[m
[32m+[m[32m  .btn-group + .btn,[m
[32m+[m[32m  .btn-group + .btn-group {[m
[32m+[m[32m    margin-left: -1px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Optional: Group multiple button groups together for a toolbar[m
[32m+[m[32m.btn-toolbar {[m
[32m+[m[32m  margin-left: -5px; // Offset the first child's margin[m
[32m+[m[32m  &:extend(.clearfix all);[m
[32m+[m
[32m+[m[32m  .btn-group,[m
[32m+[m[32m  .input-group {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .btn,[m
[32m+[m[32m  > .btn-group,[m
[32m+[m[32m  > .input-group {[m
[32m+[m[32m    margin-left: 5px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.btn-group > .btn:not(:first-child):not(:last-child):not(.dropdown-toggle) {[m
[32m+[m[32m  border-radius: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Set corners individual because sometimes a single button can be in a .btn-group and we need :first-child and :last-child to both match[m
[32m+[m[32m.btn-group > .btn:first-child {[m
[32m+[m[32m  margin-left: 0;[m
[32m+[m[32m  &:not(:last-child):not(.dropdown-toggle) {[m
[32m+[m[32m    .border-right-radius(0);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m// Need .dropdown-toggle since :last-child doesn't apply given a .dropdown-menu immediately after it[m
[32m+[m[32m.btn-group > .btn:last-child:not(:first-child),[m
[32m+[m[32m.btn-group > .dropdown-toggle:not(:first-child) {[m
[32m+[m[32m  .border-left-radius(0);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Custom edits for including btn-groups within btn-groups (useful for including dropdown buttons within a btn-group)[m
[32m+[m[32m.btn-group > .btn-group {[m
[32m+[m[32m  float: left;[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group > .btn-group:not(:first-child):not(:last-child) > .btn {[m
[32m+[m[32m  border-radius: 0;[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group > .btn-group:first-child {[m
[32m+[m[32m  > .btn:last-child,[m
[32m+[m[32m  > .dropdown-toggle {[m
[32m+[m[32m    .border-right-radius(0);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group > .btn-group:last-child > .btn:first-child {[m
[32m+[m[32m  .border-left-radius(0);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// On active and open, don't show outline[m
[32m+[m[32m.btn-group .dropdown-toggle:active,[m
[32m+[m[32m.btn-group.open .dropdown-toggle {[m
[32m+[m[32m  outline: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Sizing[m
[32m+[m[32m//[m
[32m+[m[32m// Remix the default button sizing classes into new ones for easier manipulation.[m
[32m+[m
[32m+[m[32m.btn-group-xs > .btn { &:extend(.btn-xs); }[m
[32m+[m[32m.btn-group-sm > .btn { &:extend(.btn-sm); }[m
[32m+[m[32m.btn-group-lg > .btn { &:extend(.btn-lg); }[m
[32m+[m
[32m+[m
[32m+[m[32m// Split button dropdowns[m
[32m+[m[32m// ----------------------[m
[32m+[m
[32m+[m[32m// Give the line between buttons some depth[m
[32m+[m[32m.btn-group > .btn + .dropdown-toggle {[m
[32m+[m[32m  padding-left: 8px;[m
[32m+[m[32m  padding-right: 8px;[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group > .btn-lg + .dropdown-toggle {[m
[32m+[m[32m  padding-left: 12px;[m
[32m+[m[32m  padding-right: 12px;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// The clickable button for toggling the menu[m
[32m+[m[32m// Remove the gradient and set the same inset shadow as the :active state[m
[32m+[m[32m.btn-group.open .dropdown-toggle {[m
[32m+[m[32m  .box-shadow(inset 0 3px 5px rgba(0,0,0,.125));[m
[32m+[m
[32m+[m[32m  // Show no shadow for `.btn-link` since it has no other button styles.[m
[32m+[m[32m  &.btn-link {[m
[32m+[m[32m    .box-shadow(none);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Reposition the caret[m
[32m+[m[32m.btn .caret {[m
[32m+[m[32m  margin-left: 0;[m
[32m+[m[32m}[m
[32m+[m[32m// Carets in other button sizes[m
[32m+[m[32m.btn-lg .caret {[m
[32m+[m[32m  border-width: @caret-width-large @caret-width-large 0;[m
[32m+[m[32m  border-bottom-width: 0;[m
[32m+[m[32m}[m
[32m+[m[32m// Upside down carets for .dropup[m
[32m+[m[32m.dropup .btn-lg .caret {[m
[32m+[m[32m  border-width: 0 @caret-width-large @caret-width-large;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Vertical button groups[m
[32m+[m[32m// ----------------------[m
[32m+[m
[32m+[m[32m.btn-group-vertical {[m
[32m+[m[32m  > .btn,[m
[32m+[m[32m  > .btn-group,[m
[32m+[m[32m  > .btn-group > .btn {[m
[32m+[m[32m    display: block;[m
[32m+[m[32m    float: none;[m
[32m+[m[32m    width: 100%;[m
[32m+[m[32m    max-width: 100%;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Clear floats so dropdown menus can be properly placed[m
[32m+[m[32m  > .btn-group {[m
[32m+[m[32m    &:extend(.clearfix all);[m
[32m+[m[32m    > .btn {[m
[32m+[m[32m      float: none;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .btn + .btn,[m
[32m+[m[32m  > .btn + .btn-group,[m
[32m+[m[32m  > .btn-group + .btn,[m
[32m+[m[32m  > .btn-group + .btn-group {[m
[32m+[m[32m    margin-top: -1px;[m
[32m+[m[32m    margin-left: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.btn-group-vertical > .btn {[m
[32m+[m[32m  &:not(:first-child):not(:last-child) {[m
[32m+[m[32m    border-radius: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m  &:first-child:not(:last-child) {[m
[32m+[m[32m    border-top-right-radius: @border-radius-base;[m
[32m+[m[32m    .border-bottom-radius(0);[m
[32m+[m[32m  }[m
