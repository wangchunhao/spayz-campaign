[32m+[m[32m@navbar-default-link-color:                #777;[m
[32m+[m[32m@navbar-default-link-hover-color:          #333;[m
[32m+[m[32m@navbar-default-link-hover-bg:             transparent;[m
[32m+[m[32m@navbar-default-link-active-color:         #555;[m
[32m+[m[32m@navbar-default-link-active-bg:            darken(@navbar-default-bg, 6.5%);[m
[32m+[m[32m@navbar-default-link-disabled-color:       #ccc;[m
[32m+[m[32m@navbar-default-link-disabled-bg:          transparent;[m
[32m+[m
[32m+[m[32m// Navbar brand label[m
[32m+[m[32m@navbar-default-brand-color:               @navbar-default-link-color;[m
[32m+[m[32m@navbar-default-brand-hover-color:         darken(@navbar-default-brand-color, 10%);[m
[32m+[m[32m@navbar-default-brand-hover-bg:            transparent;[m
[32m+[m
[32m+[m[32m// Navbar toggle[m
[32m+[m[32m@navbar-default-toggle-hover-bg:           #ddd;[m
[32m+[m[32m@navbar-default-toggle-icon-bar-bg:        #888;[m
[32m+[m[32m@navbar-default-toggle-border-color:       #ddd;[m
[32m+[m
[32m+[m
[32m+[m[32m// Inverted navbar[m
[32m+[m[32m// Reset inverted navbar basics[m
[32m+[m[32m@navbar-inverse-color:                      lighten(@gray-light, 15%);[m
[32m+[m[32m@navbar-inverse-bg:                         #222;[m
[32m+[m[32m@navbar-inverse-border:                     darken(@navbar-inverse-bg, 10%);[m
[32m+[m
