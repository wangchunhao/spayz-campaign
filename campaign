[32m+[m[32m@navbar-inverse-link-color:                 lighten(@gray-light, 15%);[m
[32m+[m[32m@navbar-inverse-link-hover-color:           #fff;[m
[32m+[m[32m@navbar-inverse-link-hover-bg:              transparent;[m
[32m+[m[32m@navbar-inverse-link-active-color:          @navbar-inverse-link-hover-color;[m
[32m+[m[32m@navbar-inverse-link-active-bg:             darken(@navbar-inverse-bg, 10%);[m
[32m+[m[32m@navbar-inverse-link-disabled-color:        #444;[m
[32m+[m[32m@navbar-inverse-link-disabled-bg:           transparent;[m
[32m+[m
[32m+[m[32m// Inverted navbar brand label[m
[32m+[m[32m@navbar-inverse-brand-color:                @navbar-inverse-link-color;[m
[32m+[m[32m@navbar-inverse-brand-hover-color:          #fff;[m
