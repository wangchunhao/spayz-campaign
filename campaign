[32m+[m[32m    .border-top-radius(0);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group-vertical > .btn-group:not(:first-child):not(:last-child) > .btn {[m
[32m+[m[32m  border-radius: 0;[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group-vertical > .btn-group:first-child:not(:last-child) {[m
[32m+[m[32m  > .btn:last-child,[m
[32m+[m[32m  > .dropdown-toggle {[m
[32m+[m[32m    .border-bottom-radius(0);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
