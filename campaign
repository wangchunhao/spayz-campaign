[1m+++ b/resources/assets/less/bootstrap/input-groups.less[m
[36m@@ -0,0 +1,166 @@[m
[32m+[m[32m//[m
[32m+[m[32m// Input groups[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m// Base styles[m
[32m+[m[32m// -------------------------[m
[32m+[m[32m.input-group {[m
[32m+[m[32m  position: relative; // For dropdowns[m
[32m+[m[32m  display: table;[m
[32m+[m[32m  border-collapse: separate; // prevent input groups from inheriting border styles from table cells when placed within a table[m
[32m+[m
[32m+[m[32m  // Undo padding and float of grid classes[m
[32m+[m[32m  &[class*="col-"] {[m
[32m+[m[32m    float: none;[m
[32m+[m[32m    padding-left: 0;[m
[32m+[m[32m    padding-right: 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .form-control {[m
[32m+[m[32m    // Ensure that the input is always above the *appended* addon button for[m
[32m+[m[32m    // proper border colors.[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    z-index: 2;[m
[32m+[m
[32m+[m[32m    // IE9 fubars the placeholder attribute in text inputs and the arrows on[m
[32m+[m[32m    // select elements in input groups. To fix it, we float the input. Details:[m
[32m+[m[32m    // https://github.com/twbs/bootstrap/issues/11561#issuecomment-28936855[m
[32m+[m[32m    float: left;[m
[32m+[m
[32m+[m[32m    width: 100%;[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Sizing options[m
[32m+[m[32m//[m
[32m+[m[32m// Remix the default form control sizing classes into new ones for easier[m
[32m+[m[32m// manipulation.[m
[32m+[m
[32m+[m[32m.input-group-lg > .form-control,[m
[32m+[m[32m.input-group-lg > .input-group-addon,[m
[32m+[m[32m.input-group-lg > .input-group-btn > .btn {[m
[32m+[m[32m  .input-lg();[m
[32m+[m[32m}[m
[32m+[m[32m.input-group-sm > .form-control,[m
[32m+[m[32m.input-group-sm > .input-group-addon,[m
[32m+[m[32m.input-group-sm > .input-group-btn > .btn {[m
[32m+[m[32m  .input-sm();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Display as table-cell[m
[32m+[m[32m// -------------------------[m
[32m+[m[32m.input-group-addon,[m
[32m+[m[32m.input-group-btn,[m
[32m+[m[32m.input-group .form-control {[m
[32m+[m[32m  display: table-cell;[m
[32m+[m
[32m+[m[32m  &:not(:first-child):not(:last-child) {[m
[32m+[m[32m    border-radius: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m// Addon and addon wrapper for buttons[m
[32m+[m[32m.input-group-addon,[m
[32m+[m[32m.input-group-btn {[m
[32m+[m[32m  width: 1%;[m
[32m+[m[32m  white-space: nowrap;[m
[32m+[m[32m  vertical-align: middle; // Match the inputs[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Text input groups[m
[32m+[m[32m// -------------------------[m
[32m+[m[32m.input-group-addon {[m
[32m+[m[32m  padding: @padding-base-vertical @padding-base-horizontal;[m
[32m+[m[32m  font-size: @font-size-base;[m
[32m+[m[32m  font-weight: normal;[m
[32m+[m[32m  line-height: 1;[m
[32m+[m[32m  color: @input-color;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  background-color: @input-group-addon-bg;[m
[32m+[m[32m  border: 1px solid @input-group-addon-border-color;[m
[32m+[m[32m  border-radius: @border-radius-base;[m
[32m+[m
[32m+[m[32m  // Sizing[m
[32m+[m[32m  &.input-sm {[m
[32m+[m[32m    padding: @padding-small-vertical @padding-small-horizontal;[m
[32m+[m[32m    font-size: @font-size-small;[m
[32m+[m[32m    border-radius: @border-radius-small;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.input-lg {[m
[32m+[m[32m    padding: @padding-large-vertical @padding-large-horizontal;[m
[32m+[m[32m    font-size: @font-size-large;[m
[32m+[m[32m    border-radius: @border-radius-large;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Nuke default margins from checkboxes and radios to vertically center within.[m
[32m+[m[32m  input[type="radio"],[m
[32m+[m[32m  input[type="checkbox"] {[m
[32m+[m[32m    margin-top: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Reset rounded corners[m
[32m+[m[32m.input-group .form-control:first-child,[m
[32m+[m[32m.input-group-addon:first-child,[m
[32m+[m[32m.input-group-btn:first-child > .btn,[m
[32m+[m[32m.input-group-btn:first-child > .btn-group > .btn,[m
[32m+[m[32m.input-group-btn:first-child > .dropdown-toggle,[m
[32m+[m[32m.input-group-btn:last-child > .btn:not(:last-child):not(.dropdown-toggle),[m
[32m+[m[32m.input-group-btn:last-child > .btn-group:not(:last-child) > .btn {[m
[32m+[m[32m  .border-right-radius(0);[m
[32m+[m[32m}[m
[32m+[m[32m.input-group-addon:first-child {[m
[32m+[m[32m  border-right: 0;[m
[32m+[m[32m}[m
[32m+[m[32m.input-group .form-control:last-child,[m
[32m+[m[32m.input-group-addon:last-child,[m
[32m+[m[32m.input-group-btn:last-child > .btn,[m
[32m+[m[32m.input-group-btn:last-child > .btn-group > .btn,[m
[32m+[m[32m.input-group-btn:last-child > .dropdown-toggle,[m
[32m+[m[32m.input-group-btn:first-child > .btn:not(:first-child),[m
[32m+[m[32m.input-group-btn:first-child > .btn-group:not(:first-child) > .btn {[m
[32m+[m[32m  .border-left-radius(0);[m
[32m+[m[32m}[m
[32m+[m[32m.input-group-addon:last-child {[m
[32m+[m[32m  border-left: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Button input groups[m
[32m+[m[32m// -------------------------[m
[32m+[m[32m.input-group-btn {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  // Jankily prevent input button groups from wrapping with `white-space` and[m
[32m+[m[32m  // `font-size` in combination with `inline-block` on buttons.[m
[32m+[m[32m  font-size: 0;[m
[32m+[m[32m  white-space: nowrap;[m
[32m+[m
[32m+[m[32m  // Negative margin for spacing, position for bringing hovered/focused/actived[m
[32m+[m[32m  // element above the siblings.[m
[32m+[m[32m  > .btn {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    + .btn {[m
[32m+[m[32m      margin-left: -1px;[m
[32m+[m[32m    }[m
[32m+[m[32m    // Bring the "active" button to the front[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus,[m
[32m+[m[32m    &:active {[m
[32m+[m[32m      z-index: 2;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Negative margin to only have a 1px border between the two[m
[32m+[m[32m  &:first-child {[m
[32m+[m[32m    > .btn,[m
[32m+[m[32m    > .btn-group {[m
[32m+[m[32m      margin-right: -1px;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  &:last-child {[m
[32m+[m[32m    > .btn,[m
[32m+[m[32m    > .btn-group {[m
[32m+[m[32m      margin-left: -1px;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
