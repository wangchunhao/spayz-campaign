[32m+[m[32m//== Colors[m
[32m+[m[32m//[m
[32m+[m[32m//## Gray and brand colors for use across Bootstrap.[m
[32m+[m
[32m+[m[32m@gray-base:              #000;[m
[32m+[m[32m@gray-darker:            lighten(@gray-base, 13.5%); // #222[m
[32m+[m[32m@gray-dark:              lighten(@gray-base, 20%);   // #333[m
[32m+[m[32m@gray:                   lighten(@gray-base, 33.5%); // #555[m
[32m+[m[32m@gray-light:             lighten(@gray-base, 46.7%); // #777[m
[32m+[m[32m@gray-lighter:           lighten(@gray-base, 93.5%); // #eee[m
[32m+[m
[32m+[m[32m@brand-primary:         darken(#428bca, 6.5%);[m
[32m+[m[32m@brand-success:         #5cb85c;[m
[32m+[m[32m@brand-info:            #5bc0de;[m
[32m+[m[32m@brand-warning:         #f0ad4e;[m
[32m+[m[32m@brand-danger:          #d9534f;[m
