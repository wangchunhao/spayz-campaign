[32m+[m[32m//[m
[32m+[m[32m// Jumbotron[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m.jumbotron {[m
[32m+[m[32m  padding: @jumbotron-padding (@jumbotron-padding / 2);[m
[32m+[m[32m  margin-bottom: @jumbotron-padding;[m
[32m+[m[32m  color: @jumbotron-color;[m
[32m+[m[32m  background-color: @jumbotron-bg;[m
[32m+[m
[32m+[m[32m  h1,[m
[32m+[m[32m  .h1 {[m
[32m+[m[32m    color: @jumbotron-heading-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  p {[m
[32m+[m[32m    margin-bottom: (@jumbotron-padding / 2);[m
[32m+[m[32m    font-size: @jumbotron-font-size;[m
[32m+[m[32m    font-weight: 200;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > hr {[m
[32m+[m[32m    border-top-color: darken(@jumbotron-bg, 10%);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .container &,[m
[32m+[m[32m  .container-fluid & {[m
[32m+[m[32m    border-radius: @border-radius-large; // Only round corners at higher resolutions if contained in a container[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .container {[m
[32m+[m[32m    max-width: 100%;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media screen and (min-width: @screen-sm-min) {[m
[32m+[m[32m    padding: (@jumbotron-padding * 1.6) 0;[m
[32m+[m
[32m+[m[32m    .container &,[m
