[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mclass TestCase extends Illuminate\Foundation\Testing\TestCase {[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Creates the application.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return \Illuminate\Foundation\Application[m
[32m+[m	[32m */[m
[32m+[m	[32mpublic function createApplication()[m
[32m+[m	[32m{[m
[32m+[m		[32m$app = require __DIR__.'/../bootstrap/app.php';[m
[32m+[m
[32m+[m		[32m$app->make('Illuminate\Contracts\Console\Kernel')->bootstrap();[m
[32m+[m
[32m+[m		[32mreturn $app;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
