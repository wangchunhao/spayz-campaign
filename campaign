[32m+[m[32m//[m
[32m+[m[32m// Grid system[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Container widths[m
[32m+[m[32m//[m
[32m+[m[32m// Set the container width, and override it for fixed navbars in media queries.[m
[32m+[m
[32m+[m[32m.container {[m
[32m+[m[32m  .container-fixed();[m
[32m+[m
[32m+[m[32m  @media (min-width: @screen-sm-min) {[m
[32m+[m[32m    width: @container-sm;[m
[32m+[m[32m  }[m
[32m+[m[32m  @media (min-width: @screen-md-min) {[m
[32m+[m[32m    width: @container-md;[m
[32m+[m[32m  }[m
[32m+[m[32m  @media (min-width: @screen-lg-min) {[m
[32m+[m[32m    width: @container-lg;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Fluid container[m
[32m+[m[32m//[m
[32m+[m[32m// Utilizes the mixin meant for fixed width containers, but without any defined[m
[32m+[m[32m// width for fluid, full width layouts.[m
[32m+[m
[32m+[m[32m.container-fluid {[m
[32m+[m[32m  .container-fixed();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Row[m
[32m+[m[32m//[m
[32m+[m[32m// Rows contain and clear the floats of your columns.[m
[32m+[m
[32m+[m[32m.row {[m
[32m+[m[32m  .make-row();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Columns[m
[32m+[m[32m//[m
[32m+[m[32m// Common styles for small and large grid columns[m
[32m+[m
[32m+[m[32m.make-grid-columns();[m
[32m+[m
[32m+[m
[32m+[m[32m// Extra small grid[m
[32m+[m[32m//[m
[32m+[m[32m// Columns, offsets, pushes, and pulls for extra small devices like[m
[32m+[m[32m// smartphones.[m
[32m+[m
[32m+[m[32m.make-grid(xs);[m
[32m+[m
[32m+[m
[32m+[m[32m// Small grid[m
[32m+[m[32m//[m
[32m+[m[32m// Columns, offsets, pushes, and pulls for the small device range, from phones[m
[32m+[m[32m// to tablets.[m
[32m+[m
[32m+[m[32m@media (min-width: @screen-sm-min) {[m
[32m+[m[32m  .make-grid(sm);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Medium grid[m
[32m+[m[32m//[m
[32m+[m[32m// Columns, offsets, pushes, and pulls for the desktop device range.[m
[32m+[m
[32m+[m[32m@media (min-width: @screen-md-min) {[m
[32m+[m[32m  .make-grid(md);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Large grid[m
[32m+[m[32m//[m
[32m+[m[32m// Columns, offsets, pushes, and pulls for the large desktop device range.[m
[32m+[m
[32m+[m[32m@media (min-width: @screen-lg-min) {[m
[32m+[m[32m  .make-grid(lg);[m
[32m+[m[32m}[m
