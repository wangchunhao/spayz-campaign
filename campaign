[32m+[m[32m//[m
[32m+[m[32m// Progress bars[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Bar animations[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m// WebKit[m
[32m+[m[32m@-webkit-keyframes progress-bar-stripes {[m
[32m+[m[32m  from  { background-position: 40px 0; }[m
[32m+[m[32m  to    { background-position: 0 0; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Spec and IE10+[m
[32m+[m[32m@keyframes progress-bar-stripes {[m
[32m+[m[32m  from  { background-position: 40px 0; }[m
[32m+[m[32m  to    { background-position: 0 0; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Bar itself[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m// Outer container[m
[32m+[m[32m.progress {[m
[32m+[m[32m  overflow: hidden;[m
[32m+[m[32m  height: @line-height-computed;[m
[32m+[m[32m  margin-bottom: @line-height-computed;[m
[32m+[m[32m  background-color: @progress-bg;[m
[32m+[m[32m  border-radius: @progress-border-radius;[m
[32m+[m[32m  .box-shadow(inset 0 1px 2px rgba(0,0,0,.1));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Bar of progress[m
[32m+[m[32m.progress-bar {[m
[32m+[m[32m  float: left;[m
[32m+[m[32m  width: 0%;[m
[32m+[m[32m  height: 100%;[m
[32m+[m[32m  font-size: @font-size-small;[m
[32m+[m[32m  line-height: @line-height-computed;[m
[32m+[m[32m  color: @progress-bar-color;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  background-color: @progress-bar-bg;[m
[32m+[m[32m  .box-shadow(inset 0 -1px 0 rgba(0,0,0,.15));[m
[32m+[m[32m  .transition(width .6s ease);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Striped bars[m
[32m+[m[32m//[m
[32m+[m[32m// `.progress-striped .progress-bar` is deprecated as of v3.2.0 in favor of the[m
[32m+[m[32m// `.progress-bar-striped` class, which you just add to an existing[m
[32m+[m[32m// `.progress-bar`.[m
[32m+[m[32m.progress-striped .progress-bar,[m
[32m+[m[32m.progress-bar-striped {[m
[32m+[m[32m  #gradient > .striped();[m
[32m+[m[32m  background-size: 40px 40px;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Call animation for the active one[m
[32m+[m[32m//[m
[32m+[m[32m// `.progress.active .progress-bar` is deprecated as of v3.2.0 in favor of the[m
[32m+[m[32m// `.progress-bar.active` approach.[m
[32m+[m[32m.progress.active .progress-bar,[m
[32m+[m[32m.progress-bar.active {[m
[32m+[m[32m  .animation(progress-bar-stripes 2s linear infinite);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Variations[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m.progress-bar-success {[m
[32m+[m[32m  .progress-bar-variant(@progress-bar-success-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.progress-bar-info {[m
[32m+[m[32m  .progress-bar-variant(@progress-bar-info-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.progress-bar-warning {[m
[32m+[m[32m  .progress-bar-variant(@progress-bar-warning-bg);[m
[32m+[m[32m}[m
[32m+[m
