[32m+[m[32m//[m
[32m+[m[32m// Typography[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Headings[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32mh1, h2, h3, h4, h5, h6,[m
[32m+[m[32m.h1, .h2, .h3, .h4, .h5, .h6 {[m
[32m+[m[32m  font-family: @headings-font-family;[m
[32m+[m[32m  font-weight: @headings-font-weight;[m
[32m+[m[32m  line-height: @headings-line-height;[m
[32m+[m[32m  color: @headings-color;[m
[32m+[m
[32m+[m[32m  small,[m
[32m+[m[32m  .small {[m
[32m+[m[32m    font-weight: normal;[m
[32m+[m[32m    line-height: 1;[m
[32m+[m[32m    color: @headings-small-color;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mh1, .h1,[m
[32m+[m[32mh2, .h2,[m
[32m+[m[32mh3, .h3 {[m
[32m+[m[32m  margin-top: @line-height-computed;[m
[32m+[m[32m  margin-bottom: (@line-height-computed / 2);[m
[32m+[m
[32m+[m[32m  small,[m
[32m+[m[32m  .small {[m
[32m+[m[32m    font-size: 65%;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32mh4, .h4,[m
[32m+[m[32mh5, .h5,[m
[32m+[m[32mh6, .h6 {[m
[32m+[m[32m  margin-top: (@line-height-computed / 2);[m
[32m+[m[32m  margin-bottom: (@line-height-computed / 2);[m
[32m+[m
[32m+[m[32m  small,[m
[32m+[m[32m  .small {[m
[32m+[m[32m    font-size: 75%;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mh1, .h1 { font-size: @font-size-h1; }[m
[32m+[m[32mh2, .h2 { font-size: @font-size-h2; }[m
[32m+[m[32mh3, .h3 { font-size: @font-size-h3; }[m
[32m+[m[32mh4, .h4 { font-size: @font-size-h4; }[m
[32m+[m[32mh5, .h5 { font-size: @font-size-h5; }[m
[32m+[m[32mh6, .h6 { font-size: @font-size-h6; }[m
[32m+[m
[32m+[m
[32m+[m[32m// Body text[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32mp {[m
[32m+[m[32m  margin: 0 0 (@line-height-computed / 2);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.lead {[m
[32m+[m[32m  margin-bottom: @line-height-computed;[m
[32m+[m[32m  font-size: floor((@font-size-base * 1.15));[m
[32m+[m[32m  font-weight: 300;[m
[32m+[m[32m  line-height: 1.4;[m
[32m+[m
[32m+[m[32m  @media (min-width: @screen-sm-min) {[m
[32m+[m[32m    font-size: (@font-size-base * 1.5);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Emphasis & misc[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m// Ex: (12px small font / 14px base font) * 100% = about 85%[m
[32m+[m[32msmall,[m
[32m+[m[32m.small {[m
[32m+[m[32m  font-size: floor((100% * @font-size-small / @font-size-base));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmark,[m
[32m+[m[32m.mark {[m
[32m+[m[32m  background-color: @state-warning-bg;[m
[32m+[m[32m  padding: .2em;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Alignment[m
[32m+[m[32m.text-left           { text-align: left; }[m
[32m+[m[32m.text-right          { text-align: right; }[m
[32m+[m[32m.text-center         { text-align: center; }[m
[32m+[m[32m.text-justify        { text-align: justify; }[m
[32m+[m[32m.text-nowrap         { white-space: nowrap; }[m
[32m+[m
[32m+[m[32m// Transformation[m
[32m+[m[32m.text-lowercase      { text-transform: lowercase; }[m
[32m+[m[32m.text-uppercase      { text-transform: uppercase; }[m
[32m+[m[32m.text-capitalize     { text-transform: capitalize; }[m
[32m+[m
[32m+[m[32m// Contextual colors[m
[32m+[m[32m.text-muted {[m
[32m+[m[32m  color: @text-muted;[m
[32m+[m[32m}[m
[32m+[m[32m.text-primary {[m
[32m+[m[32m  .text-emphasis-variant(@brand-primary);[m
[32m+[m[32m}[m
[32m+[m[32m.text-success {[m
[32m+[m[32m  .text-emphasis-variant(@state-success-text);[m
[32m+[m[32m}[m
[32m+[m[32m.text-info {[m
[32m+[m[32m  .text-emphasis-variant(@state-info-text);[m
[32m+[m[32m}[m
[32m+[m[32m.text-warning {[m
[32m+[m[32m  .text-emphasis-variant(@state-warning-text);[m
[32m+[m[32m}[m
[32m+[m[32m.text-danger {[m
[32m+[m[32m  .text-emphasis-variant(@state-danger-text);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Contextual backgrounds[m
[32m+[m[32m// For now we'll leave these alongside the text classes until v4 when we can[m
[32m+[m[32m// safely shift things around (per SemVer rules).[m
[32m+[m[32m.bg-primary {[m
[32m+[m[32m  // Given the contrast here, this is the only class to have its color inverted[m
[32m+[m[32m  // automatically.[m
[32m+[m[32m  color: #fff;[m
[32m+[m[32m  .bg-variant(@brand-primary);[m
[32m+[m[32m}[m
[32m+[m[32m.bg-success {[m
[32m+[m[32m  .bg-variant(@state-success-bg);[m
[32m+[m[32m}[m
[32m+[m[32m.bg-info {[m
[32m+[m[32m  .bg-variant(@state-info-bg);[m
[32m+[m[32m}[m
[32m+[m[32m.bg-warning {[m
[32m+[m[32m  .bg-variant(@state-warning-bg);[m
[32m+[m[32m}[m
[32m+[m[32m.bg-danger {[m
[32m+[m[32m  .bg-variant(@state-danger-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Page header[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m.page-header {[m
[32m+[m[32m  padding-bottom: ((@line-height-computed / 2) - 1);[m
[32m+[m[32m  margin: (@line-height-computed * 2) 0 @line-height-computed;[m
[32m+[m[32m  border-bottom: 1px solid @page-header-border-color;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Lists[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m// Unordered and Ordered lists[m
[32m+[m[32mul,[m
[32m+[m[32mol {[m
[32m+[m[32m  margin-top: 0;[m
[32m+[m[32m  margin-bottom: (@line-height-computed / 2);[m
[32m+[m[32m  ul,[m
[32m+[m[32m  ol {[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// List options[m
[32m+[m
[32m+[m[32m// Unstyled keeps list items block level, just removes default browser padding and list-style[m
[32m+[m[32m.list-unstyled {[m
[32m+[m[32m  padding-left: 0;[m
[32m+[m[32m  list-style: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Inline turns list items into inline-block[m
[32m+[m[32m.list-inline {[m
[32m+[m[32m  .list-unstyled();[m
[32m+[m[32m  margin-left: -5px;[m
[32m+[m
[32m+[m[32m  > li {[m
[32m+[m[32m    display: inline-block;[m
[32m+[m[32m    padding-left: 5px;[m
[32m+[m[32m    padding-right: 5px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Description Lists[m
[32m+[m[32mdl {[m
[32m+[m[32m  margin-top: 0; // Remove browser default[m
[32m+[m[32m  margin-bottom: @line-height-computed;[m
[32m+[m[32m}[m
[32m+[m[32mdt,[m
[32m+[m[32mdd {[m
[32m+[m[32m  line-height: @line-height-base;[m
[32m+[m[32m}[m
[32m+[m[32mdt {[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m}[m
[32m+[m[32mdd {[m
[32m+[m[32m  margin-left: 0; // Undo browser default[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Horizontal description lists[m
[32m+[m[32m//[m
[32m+[m[32m// Defaults to being stacked without any of the below styles applied, until the[m
[32m+[m[32m// grid breakpoint is reached (default of ~768px).[m
[32m+[m
[32m+[m[32m.dl-horizontal {[m
[32m+[m[32m  dd {[m
[32m+[m[32m    &:extend(.clearfix all); // Clear the floated `dt` if an empty `dd` is present[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (min-width: @grid-float-breakpoint) {[m
[32m+[m[32m    dt {[m
[32m+[m[32m      float: left;[m
[32m+[m[32m      width: (@dl-horizontal-offset - 20);[m
[32m+[m[32m      clear: left;[m
[32m+[m[32m      text-align: right;[m
[32m+[m[32m      .text-overflow();[m
[32m+[m[32m    }[m
[32m+[m[32m    dd {[m
[32m+[m[32m      margin-left: @dl-horizontal-offset;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Misc[m
[32m+[m[32m// -------------------------[m
[32m+[m
[32m+[m[32m// Abbreviations and acronyms[m
[32m+[m[32mabbr[title],[m
[32m+[m[32m// Add data-* attribute to help out our tooltip plugin, per https://github.com/twbs/bootstrap/issues/5257[m
[32m+[m[32mabbr[data-original-title] {[m
[32m+[m[32m  cursor: help;[m
[32m+[m[32m  border-bottom: 1px dotted @abbr-border-color;[m
[32m+[m[32m}[m
[32m+[m[32m.initialism {[m
[32m+[m[32m  font-size: 90%;[m
[32m+[m[32m  text-transform: uppercase;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Blockquotes[m
[32m+[m[32mblockquote {[m
[32m+[m[32m  padding: (@line-height-computed / 2) @line-height-computed;[m
[32m+[m[32m  margin: 0 0 @line-height-computed;[m
[32m+[m[32m  font-size: @blockquote-font-size;[m
[32m+[m[32m  border-left: 5px solid @blockquote-border-color;[m
[32m+[m
[32m+[m[32m  p,[m
[32m+[m[32m  ul,[m
[32m+[m[32m  ol {[m
[32m+[m[32m    &:last-child {[m
[32m+[m[32m      margin-bottom: 0;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Note: Deprecated small and .small as of v3.1.0[m
[32m+[m[32m  // Context: https://github.com/twbs/bootstrap/issues/11660[m
[32m+[m[32m  footer,[m
[32m+[m[32m  small,[m
[32m+[m[32m  .small {[m
[32m+[m[32m    display: block;[m
[32m+[m[32m    font-size: 80%; // back to default font-size[m
[32m+[m[32m    line-height: @line-height-base;[m
[32m+[m[32m    color: @blockquote-small-color;[m
[32m+[m
[32m+[m[32m    &:before {[m
[32m+[m[32m      content: '\2014 \00A0'; // em dash, nbsp[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Opposite alignment of blockquote[m
[32m+[m[32m//[m
[32m+[m[32m// Heads up: `blockquote.pull-right` has been deprecated as of v3.1.0.[m
[32m+[m[32m.blockquote-reverse,[m
[32m+[m[32mblockquote.pull-right {[m
[32m+[m[32m  padding-right: 15px;[m
[32m+[m[32m  padding-left: 0;[m
[32m+[m[32m  border-right: 5px solid @blockquote-border-color;[m
[32m+[m[32m  border-left: 0;[m
[32m+[m[32m  text-align: right;[m
[32m+[m
[32m+[m[32m  // Account for citation[m
[32m+[m[32m  footer,[m
[32m+[m[32m  small,[m
[32m+[m[32m  .small {[m
[32m+[m[32m    &:before { content: ''; }[m
[32m+[m[32m    &:after {[m
[32m+[m[32m      content: '\00A0 \2014'; // nbsp, em dash[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Addresses[m
[32m+[m[32maddress {[m
[32m+[m[32m  margin-bottom: @line-height-computed;[m
[32m+[m[32m  font-style: normal;[m
[32m+[m[32m  line-height: @line-height-base;[m
[32m+[m[32m}[m
