[32m+[m[32m//[m
[32m+[m[32m// Wells[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m.well {[m
[32m+[m[32m  #gradient > .vertical(@start-color: darken(@well-bg, 5%); @end-color: @well-bg);[m
[32m+[m[32m  border-color: darken(@well-bg, 10%);[m
[32m+[m[32m  @shadow: inset 0 1px 3px rgba(0,0,0,.05), 0 1px 0 rgba(255,255,255,.1);[m
[32m+[m[32m  .box-shadow(@shadow);[m
[32m+[m[32m}[m
