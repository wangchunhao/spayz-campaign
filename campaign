[32m+[m	[32m * Execute the console command.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return mixed[m
[32m+[m	[32m */[m
[32m+[m	[32mpublic function handle()[m
[32m+[m	[32m{[m
[32m+[m		[32m$this->comment(PHP_EOL.Inspiring::quote().PHP_EOL);[m
[32m+[m	[32m}[m
