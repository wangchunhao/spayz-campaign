[32m+[m[32m//[m
[32m+[m[32m// Badges[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Base class[m
[32m+[m[32m.badge {[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  min-width: 10px;[m
[32m+[m[32m  padding: 3px 7px;[m
[32m+[m[32m  font-size: @font-size-small;[m
[32m+[m[32m  font-weight: @badge-font-weight;[m
[32m+[m[32m  color: @badge-color;[m
[32m+[m[32m  line-height: @badge-line-height;[m
[32m+[m[32m  vertical-align: baseline;[m
[32m+[m[32m  white-space: nowrap;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  background-color: @badge-bg;[m
[32m+[m[32m  border-radius: @badge-border-radius;[m
[32m+[m
[32m+[m[32m  // Empty badges collapse automatically (not available in IE8)[m
[32m+[m[32m  &:empty {[m
[32m+[m[32m    display: none;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Quick fix for badges in buttons[m
[32m+[m[32m  .btn & {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    top: -1px;[m
[32m+[m[32m  }[m
[32m+[m[32m  .btn-xs & {[m
[32m+[m[32m    top: 0;[m
[32m+[m[32m    padding: 1px 5px;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Hover state, but only for links[m
[32m+[m[32m  a& {[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      color: @badge-link-hover-color;[m
[32m+[m[32m      text-decoration: none;[m
[32m+[m[32m      cursor: pointer;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Account for badges in navs[m
[32m+[m[32m  .list-group-item.active > &,[m
[32m+[m[32m  .nav-pills > .active > a > & {[m
[32m+[m[32m    color: @badge-active-color;[m
[32m+[m[32m    background-color: @badge-active-bg;[m
[32m+[m[32m  }[m
[32m+[m[32m  .list-group-item > & {[m
[32m+[m[32m    float: right;[m
[32m+[m[32m  }[m
[32m+[m[32m  .list-group-item > & + & {[m
[32m+[m[32m    margin-right: 5px;[m
[32m+[m[32m  }[m
[32m+[m[32m  .nav-pills > li > a > & {[m
[32m+[m[32m    margin-left: 3px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
