[32m+[m[32m// Pills[m
[32m+[m[32m// -------------------------[m
[32m+[m[32m.nav-pills {[m
[32m+[m[32m  > li {[m
[32m+[m[32m    float: left;[m
[32m+[m
[32m+[m[32m    // Links rendered as pills[m
[32m+[m[32m    > a {[m
[32m+[m[32m      border-radius: @nav-pills-border-radius;[m
[32m+[m[32m    }[m
[32m+[m[32m    + li {[m
[32m+[m[32m      margin-left: 2px;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Active state[m
[32m+[m[32m    &.active > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: @nav-pills-active-link-hover-color;[m
[32m+[m[32m        background-color: @nav-pills-active-link-hover-bg;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Stacked pills[m
[32m+[m[32m.nav-stacked {[m
[32m+[m[32m  > li {[m
[32m+[m[32m    float: none;[m
[32m+[m[32m    + li {[m
[32m+[m[32m      margin-top: 2px;[m
[32m+[m[32m      margin-left: 0; // no need for this gap between nav items[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
