[1m+++ b/gulpfile.js[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32mvar elixir = require('laravel-elixir');[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m |--------------------------------------------------------------------------[m
[32m+[m[32m | Elixir Asset Management[m
[32m+[m[32m |--------------------------------------------------------------------------[m
[32m+[m[32m |[m
[32m+[m[32m | Elixir provides a clean, fluent API for defining some basic Gulp tasks[m
[32m+[m[32m | for your Laravel application. By default, we are compiling the Less[m
[32m+[m[32m | file for our application, as well as publishing vendor resources.[m
[32m+[m[32m |[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32melixir(function(mix) {[m
[32m+[m[32m    mix.less('app.less');[m
[32m+[m[32m});[m
