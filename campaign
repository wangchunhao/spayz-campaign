[32m+[m[32m  // Exact selectors below required to override `.table-striped` and prevent[m
[32m+[m[32m  // inheritance to nested tables.[m
[32m+[m[32m  .table > thead > tr,[m
[32m+[m[32m  .table > tbody > tr,[m
[32m+[m[32m  .table > tfoot > tr {[m
[32m+[m[32m    > td.@{state},[m
[32m+[m[32m    > th.@{state},[m
[32m+[m[32m    &.@{state} > td,[m
[32m+[m[32m    &.@{state} > th {[m
[32m+[m[32m      background-color: @background;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Hover states for `.table-hover`[m
[32m+[m[32m  // Note: this is not available for cells or rows within `thead` or `tfoot`.[m
[32m+[m[32m  .table-hover > tbody > tr {[m
[32m+[m[32m    > td.@{state}:hover,[m
[32m+[m[32m    > th.@{state}:hover,[m
[32m+[m[32m    &.@{state}:hover > td,[m
[32m+[m[32m    &:hover > .@{state},[m
[32m+[m[32m    &.@{state}:hover > th {[m
[32m+[m[32m      background-color: darken(@background, 5%);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
