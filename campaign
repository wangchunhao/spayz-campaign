[32m+[m[32m      width: 30px;[m
[32m+[m[32m      height: 30px;[m
[32m+[m[32m      margin-top: -15px;[m
[32m+[m[32m      font-size: 30px;[m
[32m+[m[32m    }[m
[32m+[m[32m    .glyphicon-chevron-left,[m
[32m+[m[32m    .icon-prev {[m
[32m+[m[32m      margin-left: -15px;[m
[32m+[m[32m    }[m
[32m+[m[32m    .glyphicon-chevron-right,[m
[32m+[m[32m    .icon-next {[m
[32m+[m[32m      margin-right: -15px;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Show and left align the captions[m
[32m+[m[32m  .carousel-caption {[m
[32m+[m[32m    left: 20%;[m
[32m+[m[32m    right: 20%;[m
[32m+[m[32m    padding-bottom: 30px;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Move up the indicators[m
[32m+[m[32m  .carousel-indicators {[m
[32m+[m[32m    bottom: 20px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
